[Unit]
Description=Temperature and humidity data logger
After=multi-user.target

[Service]
Type=simple
Restart=always
WorkingDirectory=/home/pi/oss_marine_datalogger/collectors
ExecStart=/usr/bin/python3 /home/pi/oss_marine_datalogger/collectors/temp_and_humidity.py

[Install]
WantedBy=multi-user.target

