[Unit]
Description=Temperature and humidity data logger
After=multi-user.target

[Service]
Type=simple
Restart=always
User=pi
Group=pi
WorkingDirectory=/home/pi/oss_marine_datalogger/collectors
ExecStart=python3 /home/pi/oss_marine_datalogger/collectors/temp_and_humidity.py

[Install]
WantedBy=multi-user.target

