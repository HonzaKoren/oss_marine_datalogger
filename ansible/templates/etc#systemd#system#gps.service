[Unit]
Description=GPS data logger
After=multi-user.target

[Service]
Type=simple
Restart=always
WorkingDirectory=/home/pi/oss_marine_datalogger/collectors
ExecStart=source /home/pi/.virtualenvs/prod/bin/activate && python3 /home/pi/oss_marine_datalogger/collectors/gps.py

[Install]
WantedBy=multi-user.target

